{
  "semi": true,
  "useTabs": false,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@domain",
    "^@domain/(.*)$",
    "^@data",
    "^@data/(.*)$",
    "^@adapter",
    "^@adapter/(.*)$",
    "^@ui",
    "^@ui/(.*)$",
    "^@utils",
    "^@utils/(.*)$",
    "^@infrastructure",
    "^@infrastructure/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
